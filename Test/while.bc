i = 1

/* Nested while loops */
while(i <= 1){
    sum = 0
    j = i
    while(j <= 10){
        sum += j
        j = j + 1
    }
    i = i + 1
    print sum
}

n = 0
sum = 0

/* while loop with continue */
while(n <= 10){

    if(n == 5 ){
        n++
        continue
    }

    sum += n
    n = n + 1
}

print sum

n = 0

/* while loop with break */
while(1){
    print n

    if(n > 4){
        break
    }
    n = n + 1
}

