scale = 0

define fib(n){
    if(n == 0 || n == 1){
        return 1;
    }

    return fib(n - 1) + fib(n - 2);
}

fib(0)  # 1
fib(1)  # 1
fib(2)  # 2
fib(3)  # 3
fib(4)  # 5

define fact(n){

    if(n == 0){
        return 1;
    }

    return n * fact(n - 1);
}

fact(0) # 0
fact(1) # 1
fact(2) # 2
fact(3) # 6
fact(4) # 24

n       # 0