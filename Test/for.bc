/* Nested loops with continue and break */
for(i = 0; i <= 2; ++i){

    if(i == 0 || i == 1){
        continue;
    }

    for(j = 1; j <= 10; ++j){
        if(i*j == 10){
            print i*j
            break;
        }

        print i * j
    }
}


/* Loop with break; */
sum = 0
for(i = 0; 1; ++i){
   sum +=i;

   if(sum > 100){
      print sum
      print i
      break;
   }
}

